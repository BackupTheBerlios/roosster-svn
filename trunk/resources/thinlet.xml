<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- generated by ThinG, the Thinlet GUI editor -->
<panel columns="1" weightx="1" weighty="1">
    <tabbedpane action="eventTabChanged(tabbedPane)" name="tabbedPane" weightx="1" weighty="1">
        <tab name="SearchTab" text="Search">
            <panel colspan="0" columns="3" gap="2" weightx="1" weighty="1">
                <label text="Search"/>
                <textfield name="query" perform="search(this.text, searchResult)" weightx="1"/>
                <button action="search(query.text, searchResult)" text="Go"/>
                <label text=" "/>
                <table colspan="3" name="searchResult" perform="edit(item)" weighty="1">
                    <header>
                        <column name="titleHeader" text="Title" width="150"/>
                        <column name="tagsHeader" text="Tags" width="200"/>
                        <column name="noteHeader" text="Note" width="200"/>
                    </header>
                </table>
            </panel>
        </tab>
        <tab name="EditTab" text="Edit">
            <panel columns="2" gap="2" weightx="1" weighty="1">
                <label text="Edit an Entry"/>
            </panel>
        </tab>
    </tabbedpane>
</panel>
