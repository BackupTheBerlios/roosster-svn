<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- generated by ThinG, the Thinlet GUI editor -->
<panel columns="1" weightx="1" weighty="1">
    <tabbedpane action="eventTabChanged(tabbedPane)" name="tabbedPane" selected="1" weightx="1" weighty="1">
        <tab mnemonic="0" name="searchTab" text="Search">
            <panel colspan="0" columns="3" gap="2" weightx="1" weighty="1">
                <label for="queryField" mnemonic="0" text="Search"/>
                <textfield name="queryField" perform="search(this.text, searchresultTable)" weightx="1"/>
                <button action="search(queryField.text, searchresultTable)" text="Go"/>
                <label text=" "/>
                <table colspan="3" name="searchresultTable" perform="edit(item)" tooltip="Test TOOLTIP" weighty="1">
                    <header>
                        <column name="titleHeader" text="Title" width="150"/>
                        <column name="tagsHeader" text="Tags" width="200"/>
                        <column name="noteHeader" text="Note" width="200"/>
                    </header>
                </table>
            </panel>
        </tab>
        <tab mnemonic="0" name="editTab" text="Edit">
            <panel columns="2" gap="2" weightx="1" weighty="1">
                <label colspan="2" text="Edit an Entry"/>
                <separator colspan="2"/>
                <label for="urlField" text="URL"/>
                <label name="urlLabel"/>
                <label for="titleField" text="Title"/>
                <textfield name="titleField"/>
                <label for="tagsField" mnemonic="1" text="Tags"/>
                <textfield name="tagsField"/>
                <label for="noteField" mnemonic="0" text="Note" valign="top"/>
                <textarea columns="30" name="noteField" rows="4"/>
                <separator colspan="2"/>
                <label for="typeField" text="Type"/>
                <textfield name="typeField"/>
                <label for="authorField" text="Author"/>
                <textfield name="authorField"/>
                <label for="authoremailField" text="Author-Email"/>
                <textfield name="authoremailField"/>
                <label/>
                <button action="save()" halign="right" text="Save"/>
            </panel>
        </tab>
    </tabbedpane>
</panel>
