<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- generated by ThinG, the Thinlet GUI editor -->
<panel columns="1" weightx="1" weighty="1">
    <panel gap="4" top="4" bottom="4" weightx="1">
        <menubar weightx="1">
            <menu text="i18n.File" mnemonic="0">
                <menuitem text="i18n.Open" accelerator="Ctrl O" mnemonic="0"/>
                <separator/>
                <menuitem text="i18n.Exit" accelerator="Ctrl Q" action="exitRoosster()" />
            </menu>
            <menu text="i18n.Sync">
                <menuitem text="i18n.SyncDel" accelerator="Ctrl D" mnemonic="5" action="openDeliciousSync()"/>
            </menu>
            <menu text="i18n.Help">
                <menuitem text="i18n.About" action="openDialog('about_dialog')"/>
            </menu>
        </menubar>
    </panel>
    <panel scrollable="true" weightx="1" weighty="1" gap="4" top="4" left="4" bottom="4" right="4">
        <tabbedpane action="eventTabChanged(tabbedPane)" name="tabbedPane" weightx="1" weighty="1">
            <tab mnemonic="0" name="searchTab" text="i18n.Search">
                <panel colspan="0" columns="4" top="4" left="4" bottom="4" right="4" gap="4" weightx="1" weighty="1">
                    <label font="14 bold" text="i18n.Search"/>
                    <separator colspan="4" weightx="1"/>
                    <label for="queryField" text="i18n.Search"/>
                    <textfield editable="true" colspan="2" columns="50" name="queryField" perform="searchEntries('')" weightx="1"/>
                    <button action="searchEntries('')" font="bold" text="i18n.Go"/>
                    <label name="pagerLabel" colspan="2" weightx="1"/>
                    <panel colspan="2" halign="right" gap="10">
                      <label text="i18n.SortField" />
                      <combobox halign="left" editable="false" name="sortBox" action="searchEntries('')" selected="0">
                          <choice text="i18n.Relevance" property="sortfield=" />
                          <choice text="i18n.Title" property="sortfield=_titlesort" />
                          <choice text="i18n.URL" property="sortfield=url" />
                          <choice text="i18n.Author" property="sortfield=_authorsort" />
                          <choice text="i18n.AuthorEmail" property="sortfield=_authormailsort" />
                          <choice text="i18n.Type" property="sortfield=filetype" />
                          <choice text="i18n.Issued" property="sortfield=issued" />
                          <choice text="i18n.Added" property="sortfield=added" />
                          <choice text="i18n.Modified" property="sortfield=modified" />
                          <choice text="i18n.Edited" property="sortfield=edited" />
                      </combobox>
                      <panel>
                          <button text="&lt;-" name="pagerbackButton" action="searchEntries(pagersizeBox.text)" enabled="false"/>
                          <combobox editable="false" name="pagersizeBox" action="determinePagerEnabled()" selected="1">
                              <choice text="10"/>
                              <choice text="20"/>
                              <choice text="50"/>
                          </combobox>
                          <button text="-&gt;" name="pagerforwardButton" action="searchEntries(pagerforwardButton.name)" enabled="false" />
                      </panel>
                    </panel>
                    <table colspan="4" name="searchresultTable" perform="fillEditForm(item)" visible="false" weightx="1" weighty="1">
                        <header>
                            <column name="pubHeader" text="i18n.Pub" width="30"/>
                            <column name="titleHeader" text="i18n.Title" width="250"/>
                            <column name="tagsHeader" text="i18n.Tags" width="150"/>
                            <column name="noteHeader" text="i18n.Note" width="150"/>
                        </header>
                    </table>
                    <label colspan="4" name="emptyresultLabel" text="i18n.EmptyResult" visible="false"/>
                </panel>
            </tab>
            <tab mnemonic="0" name="editTab" text="i18n.Edit" enabled="false">
                <panel weightx="1" weighty="1" gap="4" top="4" left="4" bottom="4" right="4" >
                    <panel columns="2" gap="4" weightx="2" weighty="1">
                        <label font="14 bold" text="i18n.EditEntry"/>
                        <separator colspan="2"/>
                        <label text="i18n.URL"/>
                        <button name="urlButton" type="link" halign="left" action="goUrl(this)"/>
                        <label text="i18n.Tags"/>
                        <label name="tagsLabel"/>
                        <label for="titleField" text="i18n.Title"/>
                        <textfield name="titleField" perform="saveEntry()"/>
                        <label for="noteField" mnemonic="0" text="i18n.Note" valign="top"/>
                        <textarea columns="30" name="noteField" rows="4" />
                        <separator colspan="2"/>
                        <label for="typeField" text="i18n.Type"/>
                        <textfield name="typeField"  perform="saveEntry()"/>
                        <label for="authorField" text="i18n.Author"/>
                        <textfield name="authorField"  perform="saveEntry()"/>
                        <label for="authoremailField" text="i18n.AuthorEmail"/>
                        <textfield name="authoremailField"  perform="saveEntry()"/>
                        <label for="publicprivateField" text="i18n.Public"/>
                        <checkbox name="publicprivateField"/>
                        <separator colspan="2"/>
                        <label text="i18n.Actions"/>
                        <panel columns="2" gap="15" weightx="1">
                            <button name="cachedcopyButton" text="i18n.CachedCopy" action="goUrl(this)" />
                            <button name="deleteButton" foreground="#ff0000" text="i18n.DeleteEntry" action="deleteEntry()"/>
                        </panel>
                        <separator colspan="2"/>
                        <label text="i18n.Added"/>
                        <label name="addeddateLabel"/>
                        <label text="i18n.Issued"/>
                        <label name="issueddateLabel"/>
                        <label text="i18n.Modified"/>
                        <label name="modifieddateLabel"/>
                        <label text="i18n.Edited"/>
                        <label name="editeddateLabel"/>
                        <label/>
                        <button action="saveEntry()" halign="right" font="bold" text="i18n.Save"/>
                    </panel>
                    <panel columns="2" weightx="1">
                        <label colspan="2" for="tagsField" mnemonic="1" text="i18n.Tags" weightx="1"/>
                        <textfield colspan="2" insert="suggestTag(this)" name="tagsField" perform="markTag(this)"/>
                        <label colspan="2" halign="left" text="i18n.AllTags"/>
                        <list colspan="2" name="tagsList" selection="multiple" weighty="1"/>
                    </panel>
                </panel>
            </tab>
            <tab mnemonic="0" name="addTab" text="i18n.Add">
                <panel columns="4" gap="4" top="4" left="4" bottom="4" right="4">
                    <label colspan="3"  font="14 bold" text="i18n.AddEntry"/>
                    <separator colspan="4" weightx="1"/>
                    <label text="i18n.URL"/>
                    <textfield columns="50" name="addurlField" perform="addEntry()"/>
                    <button action="addEntry()" font="bold" text="i18n.AddURL"/>
                    <togglebutton action="toggleVisible(advancedaddPanel)" colspan="2" halign="left" text="i18n.AdvancedAddOptions"/>
                    <panel colspan="4" columns="2" name="advancedaddPanel" visible="false">
                        <checkbox name="addfetchcontentCheckbox" selected="true"/>
                        <label for="addfetchcontentCheckbox" text="i18n.FetchContent"/>
                        <checkbox name="addpublicCheckbox"/>
                        <label for="addpublicCheckbox" text="i18n.Public"/>
                        <checkbox name="addforceCheckbox"/>
                        <label for="addforceCheckbox" text="i18n.ForceAdd"/>
                    </panel>
                </panel>
            </tab>
        </tabbedpane>
    </panel>
    <panel name="status" columns="1" left="5" bottom="2" right="5">
        <label weightx="1" font="bold" foreground="#00df00" name="infomessagesLabel" text=" " />
        <label weightx="1" font="bold" foreground="#df0000" name="errormessagesLabel" visible="false" />    
        <label weightx="1" name="statusmessagesLabel" visible="false"/>    
    </panel>    
</panel>
